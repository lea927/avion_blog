<h1>Blog App</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
  <%= render "navbar" %>
    <% @articles.each do |article| %>
        <tr>
          <td><%= link_to article.name, article_path(article) %></td>
          <td><%= article.body %></td>
          <td></td>
        </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to "New article", new_article_path %>

