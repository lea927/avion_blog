
  <%= form_with model: @article do |f| %>
    <div class="form-group">
      <%= f.label :name, "Blog Title" %>
      <%= f.text_field :name, placeholder: "Blog Title",class: "form-control" %>
    </div>
    <div class="form-group">
      <%= f.label :body, "Content" %>
      <%= f.text_area :body, placeholder: "Blog content",class: "form-control", rows: "10" %>
    </div>
    <%= f.submit %>
  <% end %>
